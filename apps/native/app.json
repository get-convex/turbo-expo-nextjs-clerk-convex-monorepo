{
  "expo": {
    "newArchEnabled": true,
    "name": "NotesContract",
    "slug": "NotesContract",
    "version": "1.0.0",
    "scheme": "notescontract",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D87E1"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font", "expo-router"]
  }
}
